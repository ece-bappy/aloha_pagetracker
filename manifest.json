{
  "manifest_version": 3,
  "name": "Webpage Tracker",
  "version": "1.1",
  "description": "Tracks specific webpage visits with dates, includes import/export feature",
  "permissions": ["activeTab", "storage"],
  "action": {
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
}